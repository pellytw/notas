<h1>Listing delegacion_administrativas</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Localidad</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @delegacion_administrativas.each do |delegacion_administrativa| %>
  <tr>
    <td><%= delegacion_administrativa.nombre %></td>
    <td><%= delegacion_administrativa.localidad_id %></td>
    <td><%= link_to 'Show', delegacion_administrativa %></td>
    <td><%= link_to 'Edit', edit_delegacion_administrativa_path(delegacion_administrativa) %></td>
    <td><%= link_to 'Destroy', delegacion_administrativa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

