<h1>Listado de Tipos de Documentos</h1>

<table class="table table-striped table-bordered">
  <tr>
    <th>Nombre</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tipo_documentos.each do |tipo_documento| %>
  <tr>
    <td><%= tipo_documento.nombre %></td>
    <td><%= link_to 'Ver', tipo_documento, :class => "btn btn-success btn-mini" %></td>
    <td><%= link_to 'Editar', edit_tipo_documento_path(tipo_documento), :class => "btn btn-info btn-mini" %></td> 
    <td><%= link_to 'Eliminar', tipo_documento, method: :delete, data: { confirm: 'Seguro desea eliminar?' }, :class => "btn btn-danger btn-mini" %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nuevo Tipo de Documento', new_tipo_documento_path, :class => "btn btn-success"  %>
